								"exec": [
									"pm.environment.set(\"pet_id\", _.random(1, 1000));\r",
									"pm.environment.set(\"category_id\",_.random(1,500));\r",
									"var name = pm.variables.replaceIn(\"{{$randomFirstName}}\"); \r",
